   120                              <2> ; invoke calls a C function
   121                              <2> ; defaults to word (16 bit) size parameters
   122                              <2> %macro invoke 1-*
   123                              <2> %rotate -1
   124                              <2> ;%define invoketype word
   125                              <2> %rep (%0-1)
   126                              <2> ;       %ifidni %1,dword
   127                              <2> ;               %define invoketype dword
   128                              <2> ;       %elifidni %1,word
   129                              <2> ;               %define invoketype word
   130                              <2> ;       %elifidni %1,byte
   131                              <2> ;               %define invoketype byte
   132                              <2> ;       %else
   133                              <2>         %ifidni %1, cs
   134                              <2>                 o16 push %1
   135                              <2>         %elifidni %1, ds
   136                              <2>                 o16 push %1
   137                              <2>         %elifidni %1, es
   138                              <2>                 o16 push %1
   139                              <2>         %elifidni %1, fs
   140                              <2>                 o16 push %1
   141                              <2>         %elifidni %1, gs
   142                              <2>                 o16 push %1
   143                              <2>         %elifidni %1, word cs
   144                              <2>                 o16 push %1
   145                              <2>         %elifidni %1, word ds
   146                              <2>                 o16 push %1
   147                              <2>         %elifidni %1, word es
   148                              <2>                 o16 push %1
   149                              <2>         %elifidni %1, word fs
   150                              <2>                 o16 push %1
   151                              <2>         %elifidni %1, word gs
   152                              <2>                 o16 push %1
   153                              <2>         %else
   154                              <2>                 push %1
   155                              <2>         %endif
   156                              <2> ;       %endif
   157                              <2>         %rotate -1
   158                              <2> %endrep
   159                              <2> call %1
   160                              <2> %if (%0!=1)
