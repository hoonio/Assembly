    28                              <1> %include "myC32.mac"
    29                              <2> ; NASM macro set to make interfacing to 32-bit programs easier
    30                              <2> ; Also cool little macros to make NASM emulate some MASM things.
    31                              <2> ;
    32                              <2> ; Originally included in NASM.  Modifications by Peter Johnson, 1999.
    33                              <2> ;
    34                              <2> ; $Id: myC32.mac,v 1.6 2002/03/31 08:18:22 mu Exp $
    35                              <2> 
    36                              <2> %imacro proc 1                  ; begin a procedure definition
    37                              <2> %ifctx proc
    38                              <2> %error Mismatched `endproc'/`proc'
    39                              <2> %else
    40                              <2> %push proc
    41                              <2>           global %1
    42                              <2> %1:       push  ebp
    43                              <2>           mov   ebp, esp
    44                              <2> %assign %$arg 8
    45                              <2> %assign %$local 0
    46                              <2> ;%assign %$argnum 0
    47                              <2> %define %$procname %1
    48                              <2> %endif
    49                              <2> %endmacro
    50                              <2> 
    51                              <2> %imacro arg 0-1 4               ; used with the argument name as a label
    52                              <2> %00       equ %$arg
    53                              <2> ;%assign %$argnum %$argnum+1
    54                              <2> ;.arg%$argnum   equ     %1
    55                              <2> %assign %$arg %1+%$arg
    56                              <2> %endmacro
    57                              <2> 
    58                              <2> %imacro local 0-1 4		; like arg, but declare c local variables
    59                              <2> %if %$local == 0
    60                              <2>           add   esp, .STACK_FRAME_SIZE
    61                              <2> %endif
    62                              <2> %assign %$local %$local-%1
    63                              <2> %00       equ %$local
    64                              <2> %endmacro
    65                              <2> 
    66                              <2> %imacro endproc 0
    67                              <2> %ifnctx proc
    68                              <2> %error Mismatched `endproc'/`proc'
    69                              <2> %else
    70                              <2> .STACK_FRAME_SIZE	equ	%$local	; stack size known by end
    71                              <2> ;        mov     esp, ebp
    72                              <2> ;        pop     ebp
    73                              <2> %ifdef LEGACY_ENDPROC
    74                              <2>         ret
    75                              <2> %endif
    76                              <2> ;__end_%$procname:               ; useful for calculating function size
    77                              <2> ;          global %{$procname}_arglen
    78                              <2> ;%{$procname}_arglen    equ     %$arg-8
    79                              <2> ;%assign %$i 1
    80                              <2> ;%rep %$argnum
    81                              <2> ;          global %{$procname}_arg%$i
    82                              <2> ;%{$procname}_arg%$i    equ     %{$procname}.arg%$i
    83                              <2> ;%assign %$i %$i+1
    84                              <2> ;%endrep
    85                              <2> %pop
    86                              <2> %endif
    87                              <2> %endmacro
    88                              <2> 
    89                              <2> ; redefine ret instructions for in-proc cases
    90                              <2> %imacro ret 0-1
    91                              <2> %ifnctx proc
    92                              <2> 	ret	%1
    93                              <2> %else
    94                              <2> 	mov	esp, ebp
    95                              <2> 	pop	ebp
    96                              <2> 	ret	%1
    97                              <2> %endif
    98                              <2> %endmacro
    99                              <2> 
   100                              <2> %imacro retf 0-1
   101                              <2> %ifnctx proc
   102                              <2> 	retf	%1
   103                              <2> %else
